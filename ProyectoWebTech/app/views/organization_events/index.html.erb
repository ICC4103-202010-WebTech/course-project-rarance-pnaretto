<p id="notice"><%= notice %></p>

<h1>Organization Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organization_events.each do |organization_event| %>
      <tr>
        <td><%= link_to 'Show', organization_event %></td>
        <td><%= link_to 'Edit', edit_organization_event_path(organization_event) %></td>
        <td><%= link_to 'Destroy', organization_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organization Event', new_organization_event_path %>
