<p id="notice"><%= notice %></p>

<h1>Event Date Votes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_date_votes.each do |event_date_vote| %>
      <tr>
        <td><%= link_to 'Show', event_date_vote %></td>
        <td><%= link_to 'Edit', edit_event_date_vote_path(event_date_vote) %></td>
        <td><%= link_to 'Destroy', event_date_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Date Vote', new_event_date_vote_path %>
