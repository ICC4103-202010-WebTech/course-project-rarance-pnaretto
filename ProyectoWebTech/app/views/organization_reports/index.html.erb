<p id="notice"><%= notice %></p>

<h1>Organization Reports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organization_reports.each do |organization_report| %>
      <tr>
        <td><%= link_to 'Show', organization_report %></td>
        <td><%= link_to 'Edit', edit_organization_report_path(organization_report) %></td>
        <td><%= link_to 'Destroy', organization_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organization Report', new_organization_report_path %>
