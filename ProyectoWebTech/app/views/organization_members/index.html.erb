<p id="notice"><%= notice %></p>

<h1>Organization Members</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organization_members.each do |organization_member| %>
      <tr>
        <td><%= link_to 'Show', organization_member %></td>
        <td><%= link_to 'Edit', edit_organization_member_path(organization_member) %></td>
        <td><%= link_to 'Destroy', organization_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organization Member', new_organization_member_path %>
