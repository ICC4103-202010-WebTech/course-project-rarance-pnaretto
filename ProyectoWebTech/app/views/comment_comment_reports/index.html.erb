<p id="notice"><%= notice %></p>

<h1>Comment Comment Reports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comment_comment_reports.each do |comment_comment_report| %>
      <tr>
        <td><%= link_to 'Show', comment_comment_report %></td>
        <td><%= link_to 'Edit', edit_comment_comment_report_path(comment_comment_report) %></td>
        <td><%= link_to 'Destroy', comment_comment_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comment Comment Report', new_comment_comment_report_path %>
