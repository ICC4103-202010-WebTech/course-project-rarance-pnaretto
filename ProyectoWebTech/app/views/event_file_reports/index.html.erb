<p id="notice"><%= notice %></p>

<h1>Event File Reports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_file_reports.each do |event_file_report| %>
      <tr>
        <td><%= link_to 'Show', event_file_report %></td>
        <td><%= link_to 'Edit', edit_event_file_report_path(event_file_report) %></td>
        <td><%= link_to 'Destroy', event_file_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event File Report', new_event_file_report_path %>
