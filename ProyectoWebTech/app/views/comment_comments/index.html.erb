<p id="notice"><%= notice %></p>

<h1>Comment Comments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comment_comments.each do |comment_comment| %>
      <tr>
        <td><%= link_to 'Show', comment_comment %></td>
        <td><%= link_to 'Edit', edit_comment_comment_path(comment_comment) %></td>
        <td><%= link_to 'Destroy', comment_comment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comment Comment', new_comment_comment_path %>
